{
  "name": "js-on-js",
  "version": "0.0.10",
  "description": "JS written in JS",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.23.0",
    "body-parser": "^1.15.1",
    "cookie-parser": "^1.4.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.1",
    "eslint": "^3.14.0",
    "express": "^4.13.4",
    "fast-sass-loader": "^1.2.4",
    "file-loader": "^0.8.1",
    "html-loader": "^0.4.3",
    "jasmine": "^2.7.0",
    "jasmine-core": "^2.7.0",
    "karma": "^1.7.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-webpack-preprocessor": "^1.0.1",
    "mocha": "^2.4.5",
    "node-sass": "^4.5.3",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "scripts": {
    "start": "node run-demo",
    "build": "rm -rf build && ./node_modules/.bin/webpack",
    "build-ui": "rm -rf build && babel-node run-webpack-ui && cp build/index.js ../shinobi/vendor/ui-widgets/index.js && cp build/index.js ../grodno/vendor/ui/index.js",
    "reset": "rm -rf node_modules && npm run nvm && npm i",
    "jasmine": "nodemon --watch src --exec babel-node run-jasmine",
    "karma": "babel-node run-karma",
    "mocha": "mocha -w --compilers js:babel-register test/**/*.test.js",
    "lint": "node node_modules/eslint/bin/eslint.js .",
    "nvm": "nvm install v6 && nvm use v6 || echo skip nvm update...",
    "ci:build": "npm run lint && npm run test && npm run build",
    "ci:complete": "npm run reset && npm run ci:build",
    "postinstall": "echo $NODE_ENV"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alitskevich/js-on-js.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {}
}
